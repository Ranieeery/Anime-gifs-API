{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/random",
        "dest": "/index.js"
      },
      {
        "src": "/:gifName",
        "dest": "/index.js"
      }
    ],
    "env": {
      "PORT": "3000"
    }
  }
  